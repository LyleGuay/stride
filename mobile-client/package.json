{
  "name": "mobile-client",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "android:usb:local-api": "bash scripts/android-local.sh",
    "android:usb:live-api": "adb reverse tcp:8081 tcp:8081 && EXPO_PUBLIC_API_URL=https://stride.lyleguay.com/api expo start --android --localhost",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "@stride/shared": "workspace:*",
    "expo": "~55.0.4",
    "expo-router": "^55.0.3",
    "expo-secure-store": "^55.0.8",
    "expo-status-bar": "~55.0.4",
    "nativewind": "^4.2.2",
    "react": "19.2.0",
    "react-native": "0.83.2",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.2.2",
    "eslint": "^9.39.1",
    "eslint-config-expo": "^55.0.0",
    "jest-expo": "^55.0.9",
    "typescript": "~5.9.2"
  },
  "private": true
}
